# Git files
.git
.gitignore
.gitattributes

# Python cache
__pycache__/
*.py[cod]
*$py.class
*.so
.Python

# Virtual environments
venv/
env/
ENV/
.venv

# Testing
.pytest_cache/
.coverage
htmlcov/
.tox/
tests/
*.test
coverage.xml

# IDE and editor files
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store

# Documentation (not needed in container)
docs/
*.md
README.md
DEV_PROGRESS.md

# Development scripts
scripts/

# Build artifacts
build/
dist/
*.egg-info/

# Environment files
.env
.env.local
*.env

# Logs
*.log
logs/

# Temporary files
tmp/
temp/
*.tmp

# OS files
Thumbs.db
.DS_Store

# Docker files (don't copy Docker-related files into image)
Dockerfile
.dockerignore
docker-compose.yml

# CI/CD files
.github/
.gitlab-ci.yml
buildspec.yml

